  skos:narrower :c_0e4cfb77;
  skos:prefLabel "r1 In-building physical infrastructure 2016 edition – for use in england*"@en .

:c_b7bccf3d dct:modified "2020-10-27T16:23:06.700Z"^^xsd:dateTime;
  skos:narrower :c_63242aeb .

:c_0e4cfb77 a skos:Concept;
  dct:created "2020-10-27T16:25:11.673Z"^^xsd:dateTime;
  dct:modified "2020-10-27T16:27:35.347Z"^^xsd:dateTime;
  skos:broader :c_63242aeb;
  skos:definition "The following are key terms used in this document and defined in regulation 44C of the Building Regulations 2010 (as amended):"@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "appendix a: key terms (adr)"@en .

:c_67b341f4 a skos:Concept;
  dct:modified "2020-10-27T16:31:48.821Z"^^xsd:dateTime;
  skos:broader :c_0e4cfb77;
  skos:definition "A physical point, located inside or outside the building, accessible to undertakings providing or authorised to provide public communications networks, where connection to the high-speed- ready in-building physical infrastructure is made available."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "access point"@en .

:c_26e1919c a skos:Concept;
  dct:modified "2020-10-27T16:31:52.607Z"^^xsd:dateTime;
  skos:broader :c_0e4cfb77;
  skos:definition "An electronic communications network which is capable of delivering broadband access services at speeds of at least 30 Mbps."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "high-speed electronic communications network"@en .

:c_5e7bed84 a skos:Concept;
  dct:modified "2020-10-27T16:31:45.812Z"^^xsd:dateTime;
  skos:broader :c_0e4cfb77;
  skos:definition "In-building physical infrastructure intended to host elements, or enable delivery, of high-speed electronic communications networks."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "high-speed-ready in-building physical infrastructure"@en .

:c_99119e22 a skos:Concept;
  dct:modified "2020-10-27T16:31:58.708Z"^^xsd:dateTime;
  skos:broader :c_0e4cfb77;
  skos:definition "Physical infrastructure or installations at the end- user’s location, including elements under joint ownership, intended to host wired or wireless access networks, where such access networks are capable of delivering electronic communications services and connecting the building access point with the network termination point."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "in-building physical infrastructure"@en .

:c_af28f591 a skos:Concept;
  dct:modified "2020-10-27T16:31:55.572Z"^^xsd:dateTime;
  skos:broader :c_0e4cfb77;
  skos:definition "Works at the end-user’s location encompassing structural modifications of the entire in-building physical infrastructure, or of a significant part of it."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "major renovation works"@en .

:c_2a5aa469 a skos:Concept;
  skos:broader :c_0e4cfb77;
  skos:definition "A physical point at which an occupier is provided with access to high-speed electronic communications networks."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:note "NOTE: The ‘occupier’ is the subscriber to the broadband service. The termination point is typically inside the building, but may be outside the building for wireless connections."@en;
  skos:prefLabel "network termination point"@en .

