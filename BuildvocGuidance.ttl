  skos:prefLabel "limiting air permeability"@en .

:c_d64e2e84 a skos:Concept;
  dct:modified "2020-10-27T06:28:36.780Z"^^xsd:dateTime;
  skos:broader :c_aca129a3;
  skos:definition "is the Building CO2 Emission Rate expressed as kgCO2/(m2 year)."@en;
  skos:inScheme :conceptScheme_751925ed;
